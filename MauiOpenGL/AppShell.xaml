<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiOpenGL.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiOpenGL"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiOpenGL">


    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Open GL"
        ContentTemplate="{DataTemplate local:OpenGLPage}"
        Route="OpenGLPage" />

</Shell>
